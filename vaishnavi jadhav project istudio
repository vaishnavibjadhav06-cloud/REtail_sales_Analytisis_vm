   "source": [
    "import pandas as pd\n",
    "import random\n",
    "from datetime import datetime, timedelta\n",
    "\n",
    "products = [\n",
    "    (\"IT001\", \"Shampoo\", 120),\n",
    "    (\"IT002\", \"Soap\", 40),\n",
    "    (\"IT003\", \"Rice\", 500),\n",
    "    (\"IT004\", \"Toothpaste\", 90),\n",
    "    (\"IT005\", \"Biscuit\", 20),\n",
    "    (\"IT006\", \"Oil\", 180),\n",
    "    (\"IT007\", \"Milk\", 30),\n",
    "    (\"IT008\", \"Sugar\", 45),\n",
    "    (\"IT009\", \"Tea Powder\", 250),\n",
    "    (\"IT010\", \"Detergent\", 210)\n",
    "]\n",
    "\n",
    "data = []\n",
    "start_date = datetime(2024, 1, 1)\n",
    "\n",
    "for i in range(1, 501):\n",
    "    item = random.choice(products)\n",
    "    date = start_date + timedelta(days=random.randint(0, 90))\n",
    "\n",
    "    data.append([\n",
    "        1000 + i,\n",
    "        date.strftime(\"%Y-%m-%d %H:%M\"),\n",
    "        item[0],\n",
    "        item[1],\n",
    "        random.randint(1, 10),\n",
    "        item[2],\n",
    "        \"India\"\n",
    "    ])\n",
    "\n",
    "df = pd.DataFrame(data, columns=[\n",
    "    \"TransactionID\", \"TransactionTime\", \"ItemCode\",\n",
    "    \"ItemDescription\", \"NumberOfItemsPurchased\",\n",
    "    \"CostPerItem\", \"Country\"\n",
    "])\n",
    "\n",
    "df"
   ]
